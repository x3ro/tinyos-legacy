COMPONENT=TestAVboard

SENSORBOARD=AVboard

PUBLIC_TOSDIR =  ../../tos
PUBLIC_LIB = $(PUBLIC_TOSDIR)/lib
PUBLIC_INTERFACES = $(PUBLIC_TOSDIR)/interfaces
PUBLIC_SENSORBOARDS = $(PUBLIC_TOSDIR)/sensorboards
PUBLIC_INCLUDE = $(PUBLIC_TOSDIR)/include


FRAMEWORKDIR = $(PUBLIC_SENSORBOARDS)/Framework

CFLAGS += -I$(FRAMEWORKDIR) \
	 -I$(PUBLIC_INTERFACES) \
	 -I$(PUBLIC_INCLUDE) \
	 -I$(PUBLIC_SENSORBOARDS)/$(SENSORBOARD) \
	-I$(PUBLIC_LIB)/I2CBusSequence	

ifdef USE_USB
CFLAGS += -DUSE_USB=$(USE_USB)
endif

include ../Makerules

