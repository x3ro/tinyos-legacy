includes PktDef;

 configuration PeekerSPrime {
  provides interface Peek;
  provides interface StdControl;
 }

 implementation {
     components GenericCommSPRIME as EnhancedComm, PeekerM;
     
     StdControl = PeekerM.StdControl;
     PeekerM.SubControl -> EnhancedComm.Control;
     PeekerM.SendPeekMsg -> EnhancedComm.SendMsg[MSG_PEEKER];
     Peek = PeekerM.Peek;
}
