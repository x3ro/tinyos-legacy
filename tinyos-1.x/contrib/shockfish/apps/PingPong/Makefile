COMPONENT=PingPong

TOSH_DATA_LENGTH=60

ifdef PINGPONG_DEFAULT_BITRATE
	CFLAGS+=-DPINGPONG_DEFAULT_BITRATE=$(PINGPONG_DEFAULT_BITRATE)
else 
	CFLAGS+=-DPINGPONG_DEFAULT_BITRATE=76170
endif


ifdef TOSH_DATA_LENGTH
	CFLAGS+=-DTOSH_DATA_LENGTH=$(TOSH_DATA_LENGTH)
endif

ifdef PING_INITIATOR
	CFLAGS+=-DPING_INITIATOR
endif
ifdef SMOKETEST
	CFLAGS+=-DSMOKETEST
endif

include ../Makerules

